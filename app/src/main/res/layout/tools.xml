<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/palette_layout"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eee"
    >
    <LinearLayout
        android:id="@+id/palette_container"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="4"
            android:clickable="true"
            android:id="@+id/tools_veiculo"
            >
            <ImageView
                android:src="@drawable/carro"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/veiculo"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/veiculo"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>

        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="4"
            android:clickable="true"
            android:id="@+id/tools_carro"
            android:visibility="gone"
            >
            <ImageView
                android:src="@drawable/carro"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/carro"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/carro"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="4"
            android:id="@+id/tools_moto"
            android:visibility="gone"
            >
            <ImageView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:src="@drawable/motorcycle"
                android:scaleType="fitCenter" />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/motocicleta"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="4"
            android:id="@+id/tools_onibus"
            android:visibility="gone"
            >
            <ImageView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:src="@drawable/bus"
                android:scaleType="centerInside" />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/busao"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"

                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="4"
            android:id="@+id/tools_vuc"
            android:visibility="gone"
            >
            <ImageView
                android:src="@drawable/truck"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/caminhao"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/tools_bici"
            >
            <ImageView
                android:src="@drawable/bici"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/bicicleta"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/tools_pessoa"
            >
            <ImageView
                android:src="@drawable/pessoa"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/pessoa"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:id="@+id/tools_zebra"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <ImageView
                android:src="@drawable/btn_faixa"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/crosswalk"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:id="@+id/tools_poste"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="gone"
            >
            <ImageView
                android:src="@drawable/freada"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/poste"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:id="@+id/tools_obstaculo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <ImageView
                android:src="@drawable/obstaculo"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/obstaculo"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/tools_freada"
            >
            <ImageView
                android:src="@drawable/freada"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/mark"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:id="@+id/tools_trajetoria"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <ImageView
                android:src="@drawable/trajetoria"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/trajetoria"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/tools_colisao"
            >
            <ImageView

                android:src="@drawable/explode"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/collision"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:id="@+id/move_map_command"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            >
            <ImageView
                android:src="@drawable/move_map"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/movemap"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:id="@+id/edit_command"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            >
            <ImageView
                android:src="@drawable/move_car"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/edit"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
        <LinearLayout
            android:background="@android:drawable/btn_default"
            android:id="@+id/exit_command"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            >
            <ImageView
                android:src="@drawable/finish"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"        />
            <TextView
                android:paddingLeft="8dp"
                android:text="@string/exit"
                android:layout_weight="5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                />
        </LinearLayout>
    </LinearLayout>
</ScrollView>